
public class assesment1 {

	public static void main(String[] args) {
		System.out.println("hello");

	}
	public static void isPassing(double grade) {
		double passing = 65;
		grade= 75;
		
		if(grade > passing ) {
		System.out.println("you passed");	
		}
		else {
		System.out.println("you failed");	
		}
		
	
	}
}
